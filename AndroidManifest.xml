<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="de.msk.mylivetracker.client.android.pro" 
      android:versionName="1.5.0.4 AI" 
      android:versionCode="150">       
    <uses-sdk android:minSdkVersion="7" />
    <application 
        android:icon="@drawable/icon" 
        android:label="@string/app_name"
        android:name="de.msk.mylivetracker.client.android.AppPro"
    >
        <activity android:name="de.msk.mylivetracker.client.android.mainview.MainActivity"
                  android:label="@string/app_name"
                  android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name="de.msk.mylivetracker.client.android.LaunchAppBroadcast" >
			<intent-filter>
				<action android:name="android.intent.action.BOOT_COMPLETED"/>
			</intent-filter>
		</receiver> 
		<receiver android:name="de.msk.mylivetracker.client.android.remoteaccess.SmsCmdReceiver" >
			<intent-filter>
				<action android:name="android.provider.Telephony.SMS_RECEIVED" />
			</intent-filter>
		</receiver>
		<receiver android:name="de.msk.mylivetracker.client.android.remoteaccess.SmsSentStatusReceiver" >
			<intent-filter>
				<action android:name="de.msk.mylivetracker.intent.action.SMS_SENT" />
				<action android:name="de.msk.mylivetracker.intent.action.SMS_DELIVERED" />
			</intent-filter>
		</receiver>
		<activity android:name="de.msk.mylivetracker.client.android.pincodequery.PinCodeQueryActivity" 
			android:screenOrientation="portrait" />
        <activity android:name="de.msk.mylivetracker.client.android.mainview.MainDetailsActivity" 
			android:screenOrientation="portrait" />
        <activity android:name="de.msk.mylivetracker.client.android.message.MessageActivity" 
			android:screenOrientation="portrait" />
		<activity android:name="de.msk.mylivetracker.client.android.preferences.PrefsServerActivity" 
			android:screenOrientation="portrait" />
		<activity android:name="de.msk.mylivetracker.client.android.preferences.PrefsAccountActivity" 
			android:screenOrientation="portrait" />
		<activity android:name="de.msk.mylivetracker.client.android.preferences.PrefsLocalizationActivity" 
			android:screenOrientation="portrait" />
		<activity android:name="de.msk.mylivetracker.client.android.preferences.PrefsOtherActivity" 
			android:screenOrientation="portrait" />
		<activity android:name="de.msk.mylivetracker.client.android.preferences.PrefsAutoActivity" 
			android:screenOrientation="portrait" />
		<activity android:name="de.msk.mylivetracker.client.android.preferences.PrefsPinCodeQueryActivity" 
			android:screenOrientation="portrait" />
		<activity android:name="de.msk.mylivetracker.client.android.preferences.PrefsRemoteAccessActivity" 
			android:screenOrientation="portrait" />
		<activity android:name="de.msk.mylivetracker.client.android.preferences.PrefsHttpProtocolParamsActivity" 
			android:screenOrientation="portrait" />
		<activity android:name="de.msk.mylivetracker.client.android.connections.mylivetracker.ConnectToMyLiveTrackerPortalActivity" 
			android:screenOrientation="portrait" />
		<activity android:name="de.msk.mylivetracker.client.android.connections.dropbox.ConnectToDropboxActivity" 
			android:screenOrientation="portrait" />
		<activity android:name="de.msk.mylivetracker.client.android.trackexport.TrackExportActivity" 
			android:screenOrientation="portrait" />	
		<activity android:name="de.msk.mylivetracker.client.android.InfoActivity" 
			android:screenOrientation="portrait" />
		<activity
      		android:name="com.dropbox.client2.android.AuthActivity"
      		android:launchMode="singleTask"
      		android:configChanges="orientation|keyboard">
      		<intent-filter>
		        <data android:scheme="db-a73z7br0b53qrbj" />
        		<action android:name="android.intent.action.VIEW" />
        		<category android:name="android.intent.category.BROWSABLE"/>
        		<category android:name="android.intent.category.DEFAULT" />
      		</intent-filter>
    	</activity>
		<service android:name="de.msk.mylivetracker.client.android.automode.AutoService"
		    android:enabled="true"
		    android:exported="false" />
		<service android:name="de.msk.mylivetracker.client.android.upload.UploadService"
		    android:enabled="true"
		    android:exported="false" />
    </application>
    <!--  uses-permission android:name="android.permission.ACCESS_MOCK_LOCATION" /-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
	<uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
	<uses-permission android:name="android.permission.CONTROL_LOCATION_UPDATES" />
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.READ_PHONE_STATE" />
	<uses-permission android:name="android.permission.RECEIVE_SMS" />
	<uses-permission android:name="android.permission.SEND_SMS" />
	<uses-permission android:name="com.dsi.ant.permission.ANT" />
	<uses-permission android:name="com.dsi.ant.permission.ANT_ADMIN" />
</manifest>